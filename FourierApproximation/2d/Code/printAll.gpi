#! /usr/bin/gnuplot -persist
set terminal png size 1000,1000 enhanced font "Helvetica Bold, 20"
set output "t1.txt.png"

set style line 1 lt 1 linecolor rgb "red" lw 1 pt 1
set style line 2 lt 1 linecolor rgb "blue" lw 1 pt 1
set xrange [0:1]
set title "t1.txt - 5 knots "
set grid

splot  "t1.txt" using 1:2:3 ls 1 title "Interpolation Fourier Row", "t1.txt" using 1:2:4 ls 2 title "Original function", 