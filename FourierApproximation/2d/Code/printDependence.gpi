#! /usr/bin/gnuplot -persist
set terminal png size 1000,1000 enhanced font "Helvetica Bold, 20"
set output "Images/Tests/tn1.txt.png"

set style line 1 lt 1 linecolor rgb "red" lw 1 pt 1
set title "Tests/tn1.txt - 10 knots "
set grid

plot  "Tests/tn1.txt" using 1:2 ls 1 title "Dependence -log(norm) of -log(h)" with lines